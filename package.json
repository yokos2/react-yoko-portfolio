{
  "name": "wakuwork",
  "description": "Minimalistic React Framework",
  "version": "0.2.0",
  "type": "module",
  "author": "Daishi Kato",
  "repository": {
    "type": "git",
    "url": "https://github.com/dai-shi/wakuwork.git"
  },
  "main": "./dist/main.js",
  "bin": "./cli.js",
  "files": [
    "cli.js",
    "src",
    "dist"
  ],
  "scripts": {
    "compile": "swc src -d dist",
    "test": "tsc --project . --noEmit",
    "examples:01_counter": "npm run compile && WAKUWORK_CONFIG='{\"devServer\":{\"dir\":\"./examples/01_counter\"}}' ./cli.js dev",
    "examples:02_nesting": "npm run compile && WAKUWORK_CONFIG='{\"devServer\":{\"dir\":\"./examples/02_nesting\"}}' ./cli.js dev"
  },
  "license": "MIT",
  "dependencies": {
    "@swc/core": "^1.3.37"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@types/node": "^18.14.6",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "react": "0.0.0-experimental-b72ed698f-20230303",
    "react-dom": "0.0.0-experimental-b72ed698f-20230303",
    "react-server-dom-webpack": "0.0.0-experimental-b72ed698f-20230303",
    "typescript": "^5.0.1-rc"
  },
  "peerDependencies": {
    "react": "experimental",
    "react-server-dom-webpack": "experimental"
  }
}
